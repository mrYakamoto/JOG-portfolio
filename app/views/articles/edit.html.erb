<%= form_for @article do |f| %>

  <%= f.label :title %>:
  <%= f.text_field :title %> <br/>
  <%= f.label :url %>:
  <%= f.text_field :url %> <br/>
  <%= f.label :image_url %>:
  <%= f.text_field :image_url %> <br/>
  <%= f.label "publication id" %>:
  <%= f.number_field :publication_id%> <br/>

  <%= f.submit value: "submit"%>
<% end %>